{
  "name": "@weejs/plugin-environment",
  "version": "0.0.1",
  "description": "Weejs Environment Plugin",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.mjs",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.mjs",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src --ext .ts",
    "build:types": "tsc -p ./tsconfig.json --emitDeclarationOnly",
    "build:bundle": "node ./build.js --target=es2022 --platform=node",
    "build": "pnpm lint && pnpm build:bundle && pnpm build:types",
    "dev": "pnpm build:bundle -w",
    "test": "node ./tests/index.js --mode=development --type=weapp"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yangtianxia/weejs.git",
    "directory": "packages/plugin-environment"
  },
  "keywords": [
    "weejs",
    "@weejs/plugin-environment"
  ],
  "author": "yangtianxia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yangtianxia/weejs/issues"
  },
  "homepage": "https://github.com/yangtianxia/weejs#readme",
  "dependencies": {
    "@txjs/bool": "^0.0.9",
    "@txjs/shared": "^0.0.15",
    "@weejs/plugin-define": "workspace:*",
    "@weejs/plugin-utils": "workspace:*",
    "@weejs/tsconfig": "workspace:*",
    "chokidar": "^4.0.0",
    "dotenv": "^16.4.5",
    "extend": "^3.0.2",
    "minimist": "^1.2.8"
  },
  "devDependencies": {
    "@types/extend": "^3.0.4",
    "@types/minimist": "^1.2.5"
  }
}
