{
  "name": "@weejs/plugin-theme",
  "version": "0.0.1",
  "description": "Weejs Theme Plugin",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.mjs",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.mjs",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist",
    "pollen.config.mjs"
  ],
  "scripts": {
    "lint": "eslint src --ext .ts",
    "prepollen": "pollen --config pollen.config.mjs",
    "build:types": "tsc -p ./tsconfig.json --emitDeclarationOnly",
    "build:bundle": "node ./build.js --target=es2022 --platform=node",
    "build": "pnpm lint && pnpm build:bundle && pnpm build:types",
    "dev": "pnpm build:bundle -w"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yangtianxia/weejs.git",
    "directory": "packages/plugin-theme"
  },
  "keywords": [
    "weejs",
    "@weejs/plugin-theme"
  ],
  "author": "yangtianxia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yangtianxia/weejs/issues"
  },
  "homepage": "https://github.com/yangtianxia/weejs#readme",
  "dependencies": {
    "@ant-design/colors": "^7.1.0",
    "@txjs/bool": "^0.0.9",
    "@txjs/shared": "^0.0.15",
    "@weejs/plugin-define": "workspace:*",
    "@weejs/plugin-environment": "workspace:*",
    "@weejs/plugin-utils": "workspace:*",
    "@weejs/tsconfig": "workspace:*",
    "clean-css": "^5.3.3",
    "cosmiconfig": "^9.0.0",
    "extend": "^3.0.2",
    "pollen-css": "^5.0.1",
    "shelljs": "^0.8.5",
    "tinycolor2": "^1.6.0",
    "webpack": "^5.94.0"
  },
  "devDependencies": {
    "@types/clean-css": "^4.2.11",
    "@types/extend": "^3.0.4"
  }
}
